<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import ru.aofoms.util.MaskField?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="105.0" prefWidth="300.0"
      xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ru.aofoms.controller.PhoneController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label prefHeight="25.0" text="Телефон"/>
    <MaskField fx:id="numberField" mask="8-DDD-DDDDDDD"/>
    <HBox alignment="BOTTOM_RIGHT" prefHeight="35.0" prefWidth="200.0" spacing="10.0">
        <Button onAction="#cancel" mnemonicParsing="false" prefWidth="60.0" text="Отмена"/>
        <Button onAction="#ok" mnemonicParsing="false" prefWidth="60.0" text="ОК"/>
    </HBox>
</VBox>
