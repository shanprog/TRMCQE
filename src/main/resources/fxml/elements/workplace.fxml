<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ru.aofoms.controller.WorkplaceController"
            prefHeight="205.0" prefWidth="400.0">
    <padding>
        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
    </padding>
    <VBox layoutX="128.0" layoutY="14.0" prefHeight="224.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <VBox prefHeight="50.0" prefWidth="380.0">
            <Label prefHeight="25.0" text="Название организации"/>
            <TextField fx:id="nameField"/>
        </VBox>
        <VBox prefHeight="55.0" prefWidth="380.0">
            <VBox.margin>
                <Insets top="5.0"/>
            </VBox.margin>
            <Label prefHeight="25.0" text="Должность"/>
            <TextField fx:id="positionField"/>
        </VBox>
        <VBox prefHeight="55.0" prefWidth="380.0">
            <VBox.margin>
                <Insets top="5.0"/>
            </VBox.margin>
            <Label prefHeight="25.0" text="Адрес организации"/>
            <TextField fx:id="addressField"/>
        </VBox>
        <HBox alignment="CENTER_RIGHT" prefHeight="35.0" prefWidth="380.0" spacing="5.0">
            <VBox.margin>
                <Insets top="10.0"/>
            </VBox.margin>
            <Button mnemonicParsing="false" prefWidth="60.0" onAction="#cancel" text="Отмена"/>
            <Button mnemonicParsing="false" prefWidth="60.0" onAction="#ok" text="ОК"/>
        </HBox>
    </VBox>
</AnchorPane>
