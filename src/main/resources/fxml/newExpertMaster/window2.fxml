<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ru.aofoms.controller.newExpertMaster.Window2Controller"
            prefHeight="365.0" prefWidth="350.0">
    <padding>
        <Insets left="10.0" right="10.0"/>
    </padding>
    <VBox prefHeight="365.0" prefWidth="350.0"
          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <HBox prefHeight="110.0" prefWidth="330.0">
            <padding>
                <Insets top="5.0"/>
            </padding>
            <VBox prefHeight="110.0" prefWidth="300.0">
                <padding>
                    <Insets right="10.0"/>
                </padding>
                <Label prefHeight="25.0" text="Паспорта"/>
                <ListView fx:id="listPassport" onMousePressed="#editPassport" prefHeight="80.0" prefWidth="290.0"/>
            </VBox>
            <VBox prefHeight="110.0" prefWidth="30.0" spacing="5.0">
                <padding>
                    <Insets top="25.0"/>
                </padding>
                <Button mnemonicParsing="false" prefWidth="30.0" onAction="#addPassport" text="+"/>
                <Button mnemonicParsing="false" prefWidth="30.0" onAction="#delPassport" text="-"/>
            </VBox>
        </HBox>
        <HBox prefHeight="110.0" prefWidth="330.0">
            <padding>
                <Insets top="5.0"/>
            </padding>
            <VBox prefHeight="110.0" prefWidth="300.0">
                <padding>
                    <Insets right="10.0"/>
                </padding>
                <Label prefHeight="25.0" text="Телефоны"/>
                <ListView fx:id="listPhone" onMousePressed="#editPhone" prefHeight="80.0" prefWidth="290.0"/>
            </VBox>
            <VBox prefHeight="110.0" prefWidth="30.0" spacing="5.0">
                <padding>
                    <Insets top="25.0"/>
                </padding>
                <Button mnemonicParsing="false" prefWidth="30.0" onAction="#addPhone" text="+"/>
                <Button mnemonicParsing="false" prefWidth="30.0" onAction="#delPhone" text="-"/>
            </VBox>
        </HBox>
        <HBox prefHeight="110.0" prefWidth="330.0">
            <padding>
                <Insets top="5.0"/>
            </padding>
            <VBox prefHeight="110.0" prefWidth="300.0">
                <padding>
                    <Insets right="10.0"/>
                </padding>
                <Label prefHeight="25.0" text="Email"/>
                <ListView fx:id="listEmail" onMousePressed="#editEmail" prefHeight="80.0" prefWidth="290.0"/>
            </VBox>
            <VBox prefHeight="110.0" prefWidth="30.0" spacing="5.0">
                <padding>
                    <Insets top="25.0"/>
                </padding>
                <Button mnemonicParsing="false" prefWidth="30.0" onAction="#addEmail" text="+"/>
                <Button mnemonicParsing="false" prefWidth="30.0" onAction="#delEmail" text="-"/>
            </VBox>
        </HBox>
        <HBox alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="330.0" spacing="5.0">
            <VBox.margin>
                <Insets/>
            </VBox.margin>
            <padding>
                <Insets bottom="5.0" top="10.0"/>
            </padding>
            <Button mnemonicParsing="false" onAction="#cancel" prefWidth="60.0" text="Отмена"/>
            <Button mnemonicParsing="false" onAction="#back" prefWidth="60.0" text="Назад"/>
            <Button mnemonicParsing="false" onAction="#next" prefWidth="60.0" text="Далее"/>
        </HBox>
    </VBox>
</AnchorPane>
